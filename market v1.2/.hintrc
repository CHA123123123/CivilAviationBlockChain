{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off",
        "duplicate-id-active": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "highest-available-document-mode": "off",
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off",
        "image-alt": "off",
        "document-title": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "dlitem": "off",
        "definition-list": "off"
      }
    ]
  }
}